module uim.consoles.tests.constraints.contents.regexp;

import uim.consoles;

@safe:

/* * ContentsRegExp
 *
 * @internal
 */
class DContentsRegExp : DContentsBase {
    /**
     * Checks if contents contain expected
     * Params:
     * Json other Expected
     */
   bool matches(Json other) {
        return preg_match(other, this.contents) > 0;
    }
    
    // Assertion message
    override string toString() {
        return "PCRE pattern found in %s".format(this.output);
    }
    
    // @param Json other Expected
    string failureDescription(Json other) {
        return "`" ~ other ~ "` " ~ toString();
    }
}

