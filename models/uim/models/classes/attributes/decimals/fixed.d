module uim.models.classes.attributes.decimals.fixed;import uim.models;@safe:// The 64 bit fixed (4) scale numbers used by PBIclass DFixedDecimalAttribute : DDecimalAttribute {  mixin(AttributeThis!("FixedDecimal"));  // Initialization hook method.  override bool initialize(Json[string] initData = null) {    if (!super.initialize(initData)) {      return false;    }    registerPath("attributes.fixedDecimal");    return true;  }}mixin(AttributeCalls!"FixedDecimal");// TODO  unittest {    testAttribute(new DFixedDecimalAttribute);    testAttribute(FixedDecimalAttribute);}