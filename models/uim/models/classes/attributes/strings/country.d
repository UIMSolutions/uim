module uim.models.classes.attributes.strings.country;import uim.models;@safe:// means.location.countryclass DCountryAttribute : DStringAttribute {  mixin(AttributeThis!("Country"));  // Initialization hook method.  override bool initialize(Json[string] initData = null) {    if (!super.initialize(initData)) {      return false;    }    registerPath("attributes.country");    return true;  }}mixin(AttributeCalls!("Country"));///unittest {  auto attribute = new DCountryAttribute;  assert(attribute.name == "CountryAttribute");  assert(attribute.registerPath == "attributes.country");  DAttribute generalAttribute = attribute;  assert(cast(DStringAttribute) generalAttribute);  assert(!cast(DIntegerAttribute) generalAttribute);  // Json value = attribute.createValue();  // assert(cast(DStringData) value);}