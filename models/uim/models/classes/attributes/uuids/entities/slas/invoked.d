module uim.models.classes.attributes.uuids.entities.slas.invoked;import uim.models;@safe:class DSLAInvokedIdAttribute : DEntityIdAttribute {  mixin(AttributeThis!("SLAInvokedId"));  // Initialization hook method.  override bool initialize(Json[string] initData = null) {    if (!super.initialize(initData)) {      return false;    }    registerPath("attributes.slainvokedid");    return true;  }}mixin(AttributeCalls!("SLAInvokedId"));///unittest {  auto attribute = new DSLAInvokedIdAttribute;  assert(attribute.name == "SLAInvokedIdAttribute");  assert(attribute.registerPath == "attributes.slainvokedid");  DAttribute generalAttribute = attribute;  assert(cast(DSLAInvokedIdAttribute) generalAttribute);  assert(cast(DEntityIdAttribute) generalAttribute);  assert(cast(DUUIDAttribute) generalAttribute);  assert(!cast(DIntegerAttribute) generalAttribute);  // Json value = attribute.createValue();  // assert(cast(DUUIDData) value);}